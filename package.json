{
    "name": "agentic-ai-sdk",
    "version": "0.1.0",
    "description": "Cross-platform SDK for Agentic AI Serverless",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "ais": "dist/cli.js"
    },
    "scripts": {
        "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --clean",
        "dev": "npm run build -- --watch",
        "start": "node dist/cli.js",
        "compile": "pkg . --out-path binaries",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "pkg": {
        "scripts": "dist/**/*.js",
        "assets": [
            "node_modules/**/*",
            ".env"
        ],
        "targets": [
            "node18-linux-x64",
            "node18-macos-x64",
            "node18-win-x64"
        ]
    },
    "keywords": [
        "sdk",
        "cli",
        "cross-platform"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/inquirer": "^9.0.9",
        "@types/node": "^20.0.0",
        "pkg": "^5.8.1",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@google/generative-ai": "^0.21.0",
        "chalk": "^5.0.0",
        "commander": "^12.0.0",
        "dotenv": "^16.4.0",
        "inquirer": "^9.0.0",
        "ora": "^8.0.0"
    }
}